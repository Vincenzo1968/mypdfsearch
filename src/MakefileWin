
CC = gcc
CFLAGS = -Wall -W -pedantic -O3 -Wno-overlength-strings
LDFLAGS = -lm -lz -liconv
#LDFLAGS = -lm -lz
OBJS = main.o mytime.o myTree.o myGenHashTable.o myInitPredefCMapHT.o myinitarray.o myoctal.o myTernarySearchTree.o myScopeHashTable.o myobjrefqueuelist.o mydictionaryqueuelist.o mystringqueuelist.o mycontentqueuelist.o mynumstacklist.o myintqueuelist.o mydecode.o scanner.o parser.o


all: mypdfsearch.exe

mypdfsearch.exe: $(OBJS)
	$(CC) -o $@ $^ $(LDFLAGS)
#mypdfsearch.exe: main.o mytime.o myTree.o myGenHashTable.o myInitPredefCMapHT.o myinitarray.o myoctal.o myTernarySearchTree.o myScopeHashTable.o myobjrefqueuelist.o mydictionaryqueuelist.o mystringqueuelist.o mycontentqueuelist.o mynumstacklist.o myintqueuelist.o mydecode.o scanner.o parser.o
#	$(CC) -o $@ $^ $(LDFLAGS)



